pipeline {
    agent any

        stages {
	        stage('SCM') {
		            steps {
			                   git branch: 'war', url: 'https://github.com/kaviya0256/jenkins_deply.git'
					               }
						               }
							               stage('Build'){
								                steps{
										                  bat 'mvn clean'
												                    bat 'mvn install'
														                    }

																              }

																	                 stage('Deployment'){
																			          steps{
